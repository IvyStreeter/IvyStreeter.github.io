{
  "version": "20.1.8",
  "tasks": [
    {
      "label": "C/C++: clang++ build active file",
      "type": "shell",
      "command": "/opt/homebrew/opt/llvm/bin/clang++",
      "args": [
        "-std=gnu++17",
        "-fcolor-diagnostics",
        "-fansi-escape-codes",
        "-g",
        "${file}",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [
        "$gcc"
      ]
    }
  ],
  "files.associations": {
    "print": "cpp"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#97b37f",
    "activityBar.background": "#97b37f",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#ebeff3",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#97b37f",
    "statusBar.background": "#7da05f",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#64804c",
    "statusBarItem.remoteBackground": "#7da05f",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#7da05f",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#7da05f99",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#7DA05F"
}